{
  "description": "A representation of a buidlicence",
  "type": "object",
  "required": [ "id", "reference", "portalType" ],
  "properties": {
    "id": {
      "description": "unique id",
      "type": "string"
    },
    "portalType": {
      "type": "string"
    },
    "reference": {
      "type": "string"
    },
    "referenceDGATLP": {
      "type": ["string", "null"]
    },
    "licenceSubject": {
      "type": ["string", "null"]
    },
    "completionState": {
      "type": ["string", "null"]
    },
    "workLocations": {
      "type": "array",
      "items": {
        "type": "object",
          "properties": {
          "street": {
            "type": "string"
          },
          "number": {
            "type": ["string", "null"]
          },
          "postalcode": {
            "type": "string"
          },
          "locality": {
            "type": "string"
          }
        }
      }
    },
    "applicants": {
      "type": "array",
      "items": {"$ref": "Applicant.json"}
    },
    "events": {
      "type": "array",
      "items": {"$ref": "Event.json"}
    }
  }
}
